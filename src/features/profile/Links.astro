---
import { profileCard, cardTitle } from './profile.css'
import { icon } from './skill.css'
import * as styles from './links.css'
import * as fonts from '@/styles/font.css'
import { Icon } from 'astro-iconify'
---

<div class={profileCard}>
  <h1 class={`${cardTitle} ${fonts.Jost}`}>Links</h1>
  <div class={styles.linkWrapper}>
    <a href="https://github.com/aqyuki" target="_blank" class={styles.link}>
      <Icon name="mdi:github" class={icon} />
    </a>
    <a href="https://x.com/aqyuki21s" target="_blank" class={styles.link}>
      <Icon name="ri:twitter-x-fill" class={icon} />
    </a>
  </div>
</div>
